void	my_sort_int_tab(int *tab, int size)
{
  int i;
  int tmp;

  i = 0;
  while (i < (size - 1))
    {
      if (tab[i] > tab[i + 1])
	{
	  tmp = tab[i];
	  tab[i] =  tab[i + 1];
	  tab[i + 1] = tmp;
	  i = -1;
	}
      i = i +1;
    }
}
